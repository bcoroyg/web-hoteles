{
  "name": "web-hoteles",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "scss": "node-sass -o assets/css/ assets/css/",
    "less": "lessc assets/css/style.less assets/css/style.css",
    "lite:server": "lite-server",
    "watch:scss": "onchange 'assets/css/*.scss' -- npm run scss",
    "dev": "concurrently \"npm run watch:scss\" \"npm run lite:server\" ",
    "clean": "rimraf dist",
    "imagemin": "imagemin assets/img/* --out-dir dist/assets/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin precios.html -d dist --htmlmin -o dist/precios.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin",
    "build:grunt": "grunt build",
    "build:gulp": "gulp build "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcoroyg/web-hoteles.git"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bcoroyg/web-hoteles/issues"
  },
  "homepage": "https://github.com/bcoroyg/web-hoteles#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^4.6.1",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "browser-sync": "^2.27.9",
    "concurrently": "^7.1.0",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "del": "^6.0.0",
    "grunt": "^1.5.2",
    "grunt-browser-sync": "^2.2.0",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-htmlmin": "^3.1.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-uglify": "^5.2.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^3.1.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^8.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^7.0.0",
    "jit-grunt": "^0.10.0",
    "less": "^4.1.2",
    "lite-server": "^2.6.1",
    "node-sass": "^7.0.1",
